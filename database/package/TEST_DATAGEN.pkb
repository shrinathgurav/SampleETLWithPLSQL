    FUNCTION USADDRESS (FORMAT        IN CHAR,
                        MAXLENGTH     IN NUMBER,
                        NULLPERCENT   IN NUMBER)
        RETURN VARCHAR2
    IS
        XRESULT           VARCHAR2 (4000);
        XMODDIRECTION     VARCHAR2 (4000);
        XMODDESCRIPTION   VARCHAR2 (4000);
        XSTREETNAME       VARCHAR2 (4000);
        XSTREETNUMBER     PLS_INTEGER;
        XSTREETSUFFIX     VARCHAR2 (4000);
        XSTREETTYPE       PLS_INTEGER;
    BEGIN
        IF DBMS_RANDOM.VALUE (1, 100) <= NULLPERCENT
        THEN
            RETURN NULL;
        ELSE
            IF FORMAT = '1'
            THEN
                IF DBMS_RANDOM.VALUE (1, 10) > 5
                THEN
                    XMODDIRECTION :=
                        STREET_NAME_MODIFIERS_DIR (
                            TRUNC (
                                DBMS_RANDOM.VALUE (1,
                                                   MAX_STREET_MODIFIERS_DIR)));
                ELSE
                    XMODDIRECTION := '';
                END IF;


                IF DBMS_RANDOM.VALUE (1, 10) > 6
                THEN
                    XMODDESCRIPTION :=
                        STREET_NAME_MODIFIERS_DESC (
                            TRUNC (
                                DBMS_RANDOM.VALUE (1,
                                                   MAX_STREET_MODIFIERS_DESC)));
                ELSE
                    XMODDESCRIPTION := '';
                END IF;


                IF DBMS_RANDOM.VALUE (1, 10) > 5
                THEN
                    XSTREETSUFFIX :=
                        STREET_SUFFIX_FULL (
                            TRUNC (
                                DBMS_RANDOM.VALUE (1, MAX_STREET_SUFFIXES)));
                ELSE
                    XSTREETSUFFIX :=
                        STREET_SUFFIX_ABBR (
                            TRUNC (
                                DBMS_RANDOM.VALUE (1, MAX_STREET_SUFFIXES)));
                END IF;


                XSTREETTYPE := TRUNC (DBMS_RANDOM.VALUE (1, 7));

                IF XSTREETTYPE = 1
                THEN
                    XSTREETNUMBER := TRUNC (DBMS_RANDOM.VALUE (1, 1000));

                    IF MOD (XSTREETNUMBER, 10) = 1
                    THEN
                        XSTREETNAME := TO_CHAR (XSTREETNUMBER) || 'st';
                    ELSIF MOD (XSTREETNUMBER, 10) = 2
                    THEN
                        XSTREETNAME := TO_CHAR (XSTREETNUMBER) || 'nd';
                    ELSIF MOD (XSTREETNUMBER, 10) = 3
                    THEN
                        XSTREETNAME := TO_CHAR (XSTREETNUMBER) || 'rd';
                    ELSE
                        XSTREETNAME := TO_CHAR (XSTREETNUMBER) || 'th';
                    END IF;


                    IF XMODDIRECTION <> ''
                    THEN
                        XSTREETNAME :=
                               STREET_NAME_MODIFIERS_DIR (
                                   TRUNC (
                                       DBMS_RANDOM.VALUE (
                                           1,
                                           MAX_STREET_MODIFIERS_DIR)))
                            || ' '
                            || XSTREETNAME;
                    END IF;
                ELSIF XSTREETTYPE = 2
                THEN
                    XSTREETNAME :=
                        STREET_NAMES_NUMBERS (
                            TRUNC (
                                DBMS_RANDOM.VALUE (1,
                                                   MAX_STREET_NAMES_NUMBERS)));


                    IF XMODDIRECTION <> ''
                    THEN
                        XSTREETNAME :=
                               STREET_NAME_MODIFIERS_DIR (
                                   TRUNC (
                                       DBMS_RANDOM.VALUE (
                                           1,
                                           MAX_STREET_MODIFIERS_DIR)))
                            || ' '
                            || XSTREETNAME;
                    END IF;
                ELSIF XSTREETTYPE = 3
                THEN
                    XSTREETNAME :=
                        STREET_NAMES_TREES (
                            TRUNC (
                                DBMS_RANDOM.VALUE (1, MAX_STREET_NAMES_TREES)));


                    IF XMODDESCRIPTION <> ''
                    THEN
                        XSTREETNAME :=
                               STREET_NAME_MODIFIERS_DESC (
                                   TRUNC (
                                       DBMS_RANDOM.VALUE (
                                           1,
                                           MAX_STREET_MODIFIERS_DESC)))
                            || ' '
                            || XSTREETNAME;
                    END IF;

                    IF XMODDIRECTION <> ''
                    THEN
                        XSTREETNAME :=
                               STREET_NAME_MODIFIERS_DIR (
                                   TRUNC (
                                       DBMS_RANDOM.VALUE (
                                           1,
                                           MAX_STREET_MODIFIERS_DIR)))
                            || ' '
                            || XSTREETNAME;
                    END IF;
                ELSIF XSTREETTYPE = 4
                THEN
                    XSTREETNAME :=
                        STREET_NAMES_STONES (
                            TRUNC (
                                DBMS_RANDOM.VALUE (1,
                                                   MAX_STREET_NAMES_STONES)));


                    IF XMODDIRECTION <> ''
                    THEN
                        XSTREETNAME :=
                               STREET_NAME_MODIFIERS_DIR (
                                   TRUNC (
                                       DBMS_RANDOM.VALUE (
                                           1,
                                           MAX_STREET_MODIFIERS_DIR)))
                            || ' '
                            || XSTREETNAME;
                    END IF;
                ELSIF XSTREETTYPE = 5
                THEN
                    XSTREETNAME :=
                        STREET_NAMES_GEO (
                            TRUNC (
                                DBMS_RANDOM.VALUE (1, MAX_STREET_NAMES_GEO)));


                    IF XMODDESCRIPTION <> ''
                    THEN
                        XSTREETNAME :=
                               STREET_NAME_MODIFIERS_DESC (
                                   TRUNC (
                                       DBMS_RANDOM.VALUE (
                                           1,
                                           MAX_STREET_MODIFIERS_DESC)))
                            || ' '
                            || XSTREETNAME;
                    END IF;

                    IF XMODDIRECTION <> ''
                    THEN
                        XSTREETNAME :=
                               STREET_NAME_MODIFIERS_DIR (
                                   TRUNC (
                                       DBMS_RANDOM.VALUE (
                                           1,
                                           MAX_STREET_MODIFIERS_DIR)))
                            || ' '
                            || XSTREETNAME;
                    END IF;
                ELSE
                    XSTREETNAME :=
                        US_STATES_FULL (
                            TRUNC (DBMS_RANDOM.VALUE (1, MAX_US_STATES)));
                END IF;


                XSTREETNUMBER := TRUNC (DBMS_RANDOM.VALUE (100, 9999));


                XRESULT :=
                       TO_CHAR (XSTREETNUMBER)
                    || ' '
                    || XSTREETNAME
                    || ' '
                    || XSTREETSUFFIX;
            ELSIF FORMAT = '2'
            THEN
                XRESULT :=
                    STREET_UNIT_NAMES (
                        TRUNC (DBMS_RANDOM.VALUE (1, MAX_STREET_UNIT_NAMES)));

                IF SUBSTR (XRESULT, 1, 1) = '#'
                THEN
                    XRESULT :=
                           XRESULT
                        || TO_CHAR (TRUNC (DBMS_RANDOM.VALUE (1, 999)));
                ELSE
                    XRESULT :=
                           XRESULT
                        || ' '
                        || TO_CHAR (TRUNC (DBMS_RANDOM.VALUE (1, 999)));
                END IF;
            ELSIF LOWER (FORMAT) = 'p'
            THEN
                XRESULT :=
                       'P.O. Box '
                    || TO_CHAR (TRUNC (DBMS_RANDOM.VALUE (1, 99999)));
            ELSIF LOWER (FORMAT) = 'f'
            THEN
                XRESULT :=
                       USADDRESS ('1', MAXLENGTH, 0)
                    || ' '
                    || USADDRESS ('2', MAXLENGTH, 0);
            ELSE
                XRESULT := NULL;
            END IF;

            RETURN SUBSTR (XRESULT, 1, LEAST (LENGTH (XRESULT), MAXLENGTH));
        END IF;
    END;



