                DEPARTMENT_NAMES (
                    TRUNC (DBMS_RANDOM.VALUE (1, MAX_DEPARTMENTS)));

            RETURN SUBSTR (XRESULT, 1, LEAST (LENGTH (XRESULT), MAXLENGTH));
        END IF;
    END;


    FUNCTION EMAIL (MAXLENGTH IN NUMBER, NULLPERCENT IN NUMBER)
        RETURN VARCHAR2
    IS
        XRESULT   VARCHAR2 (4000);
        XUSERID   VARCHAR (4000);
    BEGIN
        IF DBMS_RANDOM.VALUE (1, 100) <= NULLPERCENT
        THEN
            RETURN NULL;
        ELSE
            IF DBMS_RANDOM.VALUE (1, 10) > 7
            THEN
                XUSERID := NICKNAME ('x', MAXLENGTH, 0);
            ELSE
                XUSERID := USERID ('x', MAXLENGTH, 0);
            END IF;

            IF DBMS_RANDOM.VALUE (1, 10) > 7
            THEN
                XRESULT :=
                    LOWER (
                           XUSERID
                        || '@'
                        || REPLACE (COMPANYNAME (MAXLENGTH, 0), ' ', '')
                        || '.'
                        || COUNTRYCODE (2, 0));
            ELSE
                XRESULT :=
                    LOWER (
                           XUSERID
                        || '@'
                        || REPLACE (COMPANYNAME (MAXLENGTH, 0), ' ', '')
                        || '.'
                        || TOP_LEVEL_DOMAINS (
                               TRUNC (
                                   DBMS_RANDOM.VALUE (1,
                                                      MAX_TOP_LEVEL_DOMAINS))));
            END IF;

            RETURN SUBSTR (XRESULT, 1, LEAST (LENGTH (XRESULT), MAXLENGTH));
        END IF;
    END;



    FUNCTION FIRSTNAME (GENDER        IN CHAR,
                        MAXLENGTH     IN NUMBER,
                        NULLPERCENT   IN NUMBER)
        RETURN VARCHAR2
    IS
        XRESULT   VARCHAR2 (4000);
    BEGIN
        IF DBMS_RANDOM.VALUE (1, 100) <= NULLPERCENT
        THEN
            RETURN NULL;
        ELSE
            IF LOWER (GENDER) = 'f'
            THEN
                XRESULT :=
                    FEMALE_NAMES (TRUNC (DBMS_RANDOM.VALUE (1, MAX_NAMES)));
            ELSIF LOWER (GENDER) = 'm'
            THEN
                XRESULT :=
                    MALE_NAMES (TRUNC (DBMS_RANDOM.VALUE (1, MAX_NAMES)));
            ELSIF LOWER (GENDER) = 'x'
            THEN
                IF DBMS_RANDOM.VALUE (1, 10) > 5
                THEN
                    XRESULT :=
                        FEMALE_NAMES (
                            TRUNC (DBMS_RANDOM.VALUE (1, MAX_NAMES)));
                ELSE
                    XRESULT :=
                        MALE_NAMES (TRUNC (DBMS_RANDOM.VALUE (1, MAX_NAMES)));
                END IF;
            ELSE
                XRESULT := NULL;
            END IF;

            RETURN SUBSTR (XRESULT, 1, LEAST (LENGTH (XRESULT), MAXLENGTH));
        END IF;
    END;



    FUNCTION FULLNAME (GENDER        IN CHAR,
                       FORMAT        IN VARCHAR2,
                       MAXLENGTH     IN NUMBER,
                       NULLPERCENT   IN NUMBER)
        RETURN VARCHAR2
    IS
        XRESULT       VARCHAR2 (4000);
        XFIRSTNAME    VARCHAR2 (4000);
        XMIDDLENAME   VARCHAR2 (4000);
        XLASTNAME     VARCHAR2 (4000);
    BEGIN
        IF DBMS_RANDOM.VALUE (1, 100) <= NULLPERCENT
        THEN
            RETURN NULL;
        ELSE
            IF (LOWER (GENDER) = 'f')
            THEN
                XFIRSTNAME := FIRSTNAME (GENDER, MAXLENGTH, 0);
                XMIDDLENAME := FIRSTNAME (GENDER, MAXLENGTH, 0);
            ELSIF LOWER (GENDER) = 'm'
            THEN
                XFIRSTNAME := FIRSTNAME (GENDER, MAXLENGTH, 0);
                XMIDDLENAME := FIRSTNAME (GENDER, MAXLENGTH, 0);
            ELSIF LOWER (GENDER) = 'x'
            THEN
                IF DBMS_RANDOM.VALUE (1, 10) > 5
                THEN
                    XFIRSTNAME := FIRSTNAME ('f', MAXLENGTH, 0);
                    XMIDDLENAME := FIRSTNAME ('f', MAXLENGTH, 0);
                ELSE
                    XFIRSTNAME := FIRSTNAME ('m', MAXLENGTH, 0);
                    XMIDDLENAME := FIRSTNAME ('m', MAXLENGTH, 0);
                END IF;
            ELSE
                XRESULT := NULL;
            END IF;

            XLASTNAME := LASTNAME (MAXLENGTH, 0);

            IF LOWER (FORMAT) = 'fl'
            THEN
                XRESULT := XFIRSTNAME || ' ' || XLASTNAME;
            ELSIF LOWER (FORMAT) = 'fml'
            THEN
                XRESULT :=
                    XFIRSTNAME || ' ' || XMIDDLENAME || ' ' || XLASTNAME;
            ELSIF LOWER (FORMAT) = 'fmil'
            THEN
                XRESULT :=
                       XFIRSTNAME
                    || ' '
                    || SUBSTR (XMIDDLENAME, 1, 1)
                    || ' '
                    || XLASTNAME;
            ELSE
                XRESULT := NULL;
            END IF;

            RETURN SUBSTR (XRESULT, 1, LEAST (LENGTH (XRESULT), MAXLENGTH));
        END IF;
    END;


