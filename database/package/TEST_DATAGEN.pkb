    FUNCTION USERID (FORMAT        IN CHAR,
                     MAXLENGTH     IN NUMBER,
                     NULLPERCENT   IN NUMBER)
        RETURN VARCHAR2
    IS
        XCHOICE   PLS_INTEGER;
        XRESULT   VARCHAR2 (4000);
    BEGIN
        IF DBMS_RANDOM.VALUE (1, 100) <= NULLPERCENT
        THEN
            RETURN NULL;
        ELSE
            IF LOWER (FORMAT) = '1'
            THEN
                XRESULT :=
                       FIRSTNAME ('x', MAXLENGTH, 0)
                    || '.'
                    || LASTNAME (MAXLENGTH, 0);
            ELSIF LOWER (FORMAT) = '2'
            THEN
                XRESULT :=
                       SUBSTR (FIRSTNAME ('x', MAXLENGTH, 0), 1, 1)
                    || LASTNAME (MAXLENGTH, 0);
            ELSIF LOWER (FORMAT) = '3'
            THEN
                XRESULT :=
                       FIRSTNAME ('x', MAXLENGTH, 0)
                    || SUBSTR (LASTNAME (MAXLENGTH, 0), 1, 1);
            ELSIF LOWER (FORMAT) = '4'
            THEN
                XRESULT :=
                       SUBSTR (FIRSTNAME ('x', MAXLENGTH, 0), 1, 1)
                    || '.'
                    || LASTNAME (MAXLENGTH, 0);
            ELSIF LOWER (FORMAT) = 'x'
            THEN
                XCHOICE := TRUNC (DBMS_RANDOM.VALUE (1, 5));

                IF XCHOICE = 1
                THEN
                    XRESULT :=
                           FIRSTNAME ('x', MAXLENGTH, 0)
                        || '.'
                        || LASTNAME (MAXLENGTH, 0);
                ELSIF XCHOICE = 2
                THEN
                    XRESULT :=
                           SUBSTR (FIRSTNAME ('x', MAXLENGTH, 0), 1, 1)
                        || LASTNAME (MAXLENGTH, 0);
                ELSIF XCHOICE = 3
                THEN
                    XRESULT :=
                           FIRSTNAME ('x', MAXLENGTH, 0)
                        || SUBSTR (LASTNAME (MAXLENGTH, 0), 1, 1);
                ELSE
                    XRESULT :=
                           SUBSTR (FIRSTNAME ('x', MAXLENGTH, 0), 1, 1)
                        || '.'
                        || LASTNAME (MAXLENGTH, 0);
                END IF;
            ELSE
                XRESULT := NULL;
            END IF;

            RETURN LOWER (
                       SUBSTR (XRESULT,
                               1,
                               LEAST (LENGTH (XRESULT), MAXLENGTH)));
        END IF;
    END;



    FUNCTION USPHONE (FORMAT IN CHAR, NULLPERCENT IN NUMBER)
        RETURN VARCHAR2
    IS
        XRESULT   VARCHAR2 (4000);
    BEGIN
        IF DBMS_RANDOM.VALUE (1, 100) <= NULLPERCENT
        THEN
            RETURN NULL;
        ELSE
            IF LOWER (FORMAT) = 'd'
            THEN
                XRESULT :=
                       TO_CHAR (TRUNC (DBMS_RANDOM.VALUE (201, 990)))
                    || '-'
                    || TO_CHAR (TRUNC (DBMS_RANDOM.VALUE (201, 999)))
                    || '-'
                    || TO_CHAR (TRUNC (DBMS_RANDOM.VALUE (1000, 9999)));
            ELSIF LOWER (FORMAT) = 'p'
            THEN
                XRESULT :=
                       '('
                    || TO_CHAR (TRUNC (DBMS_RANDOM.VALUE (201, 990)))
                    || ') '
                    || TO_CHAR (TRUNC (DBMS_RANDOM.VALUE (201, 999)))
                    || '-'
                    || TO_CHAR (TRUNC (DBMS_RANDOM.VALUE (1000, 9999)));
            ELSIF LOWER (FORMAT) = 's'
            THEN
                XRESULT :=
                       TO_CHAR (TRUNC (DBMS_RANDOM.VALUE (201, 990)))
                    || '/'
                    || TO_CHAR (TRUNC (DBMS_RANDOM.VALUE (201, 999)))
                    || '-'
                    || TO_CHAR (TRUNC (DBMS_RANDOM.VALUE (1000, 9999)));
            ELSIF LOWER (FORMAT) = 'n'
            THEN
                XRESULT :=
                       TO_CHAR (TRUNC (DBMS_RANDOM.VALUE (201, 990)))
                    || TO_CHAR (TRUNC (DBMS_RANDOM.VALUE (201, 999)))
                    || TO_CHAR (TRUNC (DBMS_RANDOM.VALUE (1000, 9999)));
            ELSE
                XRESULT := NULL;
            END IF;

            RETURN XRESULT;
        END IF;
    END;



